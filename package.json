{
  "dependencies": {
    "bulma": "^0.1.2"
  },
  "devDependencies": {
    "browser-sync": "^2.14.0",
    "harp": "github:sintaxi/harp#v0.21.0-pre"
  },
  "scripts": {
    "dev": "harp server --port 9001 & browser-sync start --proxy 'localhost:9001' --files '**/*.jade, **/*.scss, **/*.json, **/*.js' --no-notify",
    "build": "rm -rf www/ && harp compile ."
  }
}
